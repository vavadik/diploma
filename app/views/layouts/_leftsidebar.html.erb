<ul class="nav nav-list">
  	<% leftside_bar(add_links).each do |link| %>
	  	<% if link[:header] %>
	  		<li class="nav-header"><%= link[:text] %></li>
	 	<% else %>
			<li class="<%= link[:name] == current ? 'active' : '' %>" >
				<%= link_to link[:text], link[:link] %>
			</li>
	  	<% end %>
  	<% end %>
</ul>